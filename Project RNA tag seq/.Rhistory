ggplot(data = dc,
aes(x = logFC,
y = log10,
colour=diffexpressed)) +
geom_point(alpha=0.25, size=3.5) +
scale_color_manual(values=c("orange2", "grey","blue3"))+
xlim(c(-2.5, 2.5)) +
ylim(0,4)+
